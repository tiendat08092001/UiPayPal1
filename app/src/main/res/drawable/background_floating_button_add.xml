<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="180"
                android:endColor="#0054ae"
                android:startColor="#0262e9" />

            <size
                android:width="24dp"
                android:height="24dp" />
        </shape>

    </item>


    <item
        android:bottom="15dp"
        android:drawable="@drawable/ic_baseline_add_24"
        android:left="15dp"
        android:right="15dp"
        android:top="15dp" />
</layer-list>